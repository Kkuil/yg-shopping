<swiper class="Swiper" autoplay indicator-dots circular>
    <swiper-item
    wx:for="{{swiperList}}"
    wx:key="goods_id"
    >
        <navigator url="{{item.navigator_url}}" open-type="{{item.open_type}}">
            <image mode="widthFix" class="img" src="{{item.image_src}}"/>
        </navigator>
    </swiper-item>
</swiper>
